      "path": "../../algorithm"
    },
    {
      "path": "../../coreutils"
    },
    {
      "path": "../../disposable"
    },
    {
      "path": "../../domutils"
    },
    {
      "path": "../../dragdrop"
    },
    {
      "path": "../../messaging"
    },
    {
      "path": "../../signaling"
    },
    {
      "path": "../../widgets"
    }
  ]
}
