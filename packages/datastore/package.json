    "test:ie": "cd tests && karma start --browsers=IE",
    "build:test": "tsc --build tests && cd tests && webpack",
    "watch": "tsc --build --watch"
  },
  "dependencies": {
    "@phosphor/algorithm": "^1.1.2",
    "@phosphor/collections": "^1.1.2",
    "@phosphor/coreutils": "^1.3.0",
